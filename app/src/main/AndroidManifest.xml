<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mike.amina.ritu">

    <!-- Include required permissions for Google Mobile Ads to run. -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mike_bro"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/mike_bro"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mike.amina.ritu.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- This meta-data tag is required to use Google Play Services. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <!-- Include the AdActivity configChanges and theme. -->
        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />
        <activity
            android:name="com.mike.amina.ritu.Banner"
            android:label="@string/title_activity_banner" />
        <activity
            android:name="com.mike.amina.ritu.Interstial"
            android:label="@string/title_activity_interstial" />
        <activity android:name="com.mike.amina.ritu.Reward" />
        <activity
            android:name="com.mike.amina.ritu.Banner_nd"
            android:label="@string/title_activity_banner_nd" />
        <activity
            android:name="com.mike.amina.ritu.Banner_rd"
            android:label="@string/title_activity_banner_rd" />
        <activity android:name="com.mike.amina.ritu.Rules"
            android:label="@string/title_activity_rules"  />
    </application>

</manifest>